{
  "name": "json-scrambler",
  "version": "1.0.0",
  "description": "Mutates the structure of a JSON object while remaining valid JSON",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "type": "module",
  "scripts": {
    "build": "rm -rf dist/* && tsup",
    "dev": "tsup --watch",
    "test": "tap run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbarre/json-scrambler.git"
  },
  "keywords": [
    "JSON",
    "testing",
    "fuzzing"
  ],
  "author": "Seb Barre <sbarre@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sbarre/json-scrambler/issues"
  },
  "homepage": "https://github.com/sbarre/json-scrambler#readme",
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/randomstring": "^1.1.11",
    "dotenv": "^16.4.1",
    "semantic-release": "^23.0.0",
    "sinon": "^17.0.1",
    "tap": "^18.7.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "tsup": {
    "dts": true,
    "bundle": false,
    "treeshake": true,
    "target": "node18",
    "format": [
      "esm",
      "cjs"
    ],
    "entry": [
      "lib/*.ts"
    ]
  },
  "dependencies": {
    "randomstring": "^1.3.0"
  }
}